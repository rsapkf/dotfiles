URxvt.scrollBar: false
URxvt.cursorColor: white
URxvt.transparent: false
URxvt.shading: 20
URxvt*cutchars: `"()'*<>[]{|}"

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: 0
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter

URxvt*loginshell: true
URxvt*savelines: 0

URxvt*imLocale: en_US.UTF-8
Urxvt*buffered: false
URxvt.intensityStyles: false

xterm.termName: xterm-256color

! Required for Source Code Pro to work with powerline fonts on rxvt-unicode(https://powerline.readthedocs.io/en/master/tips-and-tricks.html#rxvt-unicode)
URxvt*font: xft:Source\ Code\ Pro\ Medium:pixelsize=18:antialias=true:hinting=true,xft:Source\ Code\ Pro\ Medium:pixelsize=18:antialias=true:hinting=true
! URxvt*font: xft:Fira\ Code\ Light

!! Extensions
URxvt.perl-ext-common: default,matcher,keyboard-select,fullscreen,font-size
!! fullscreen
URxvt.keysym.F11: perl:fullscreen:switch
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.keysym.C-Down:  resize-font:smaller
URxvt.keysym.C-Up:    resize-font:bigger
!! Matcher
URxvt.url-launcher: /opt/firefox_dev/firefox/firefox
URxvt.matcher.button: 1
!! tabbed
!! URxvt.tabbed.new-button: false
!! URxvt.tabbed.tabbar-fg: 0
!! URxvt.tabbed.tabbar-bg: 5
!! URxvt.tabbed.tab-fg: 3
!! URxvt.tabbed.tab-bg: 0

