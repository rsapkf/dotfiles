[Desktop Entry]
Name=Mark Text
Comment=Next generation markdown editor
Exec=/opt/marktext-x86_64.AppImage %F
Terminal=false
Type=Application
Icon=marktext
Categories=Office;TextEditor;Utility;
MimeType=text/markdown;
Keywords=marktext;
StartupWMClass=marktext
Actions=NewWindow;

[Desktop Action NewWindow]
Name=New Window
Exec=/opt/marktext-x86_64.AppImage --new-window %F
